{
  "name": "@app/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nx run-many --target=build --all --parallel",
    "build:agent-api": "nx build agent-api",
    "build:dashboard-api": "nx build dashboard-api",
    "build:agent-gateway": "nx build agent-gateway",
    "build:device-adapter": "nx build device-adapter-platform",
    "serve": "nx run-many --target=serve --projects=agent-api,dashboard-api --parallel",
    "serve:agent-api": "nx serve agent-api",
    "serve:dashboard-api": "nx serve dashboard-api",
    "serve:agent-gateway": "nx serve agent-gateway",
    "serve:device-adapter": "nx serve device-adapter-platform",
    "dev": "concurrently \"npm run serve:dashboard-api\" \"npm run serve:agent-api\"",
    "dev:all": "concurrently \"npm run serve:dashboard-api\" \"npm run serve:agent-api\" \"npm run serve:agent-gateway\"",
    "db:generate": "prisma generate --schema=shared/database/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=shared/database/prisma/schema.prisma",
    "db:migrate:deploy": "prisma migrate deploy --schema=shared/database/prisma/schema.prisma",
    "db:migrate:reset": "prisma migrate reset --schema=shared/database/prisma/schema.prisma",
    "db:push": "prisma db push --schema=shared/database/prisma/schema.prisma",
    "db:seed": "ts-node shared/database/prisma/seed.ts",
    "db:studio": "prisma studio --schema=shared/database/prisma/schema.prisma",
    "db:setup": "npm run db:generate && npm run db:migrate && npm run db:seed",
    "lint": "nx run-many --target=lint --all --parallel",
    "lint:fix": "nx run-many --target=lint --all --parallel --fix",
    "test": "nx run-many --target=test --all --parallel",
    "test:watch": "nx run-many --target=test --all --parallel --watch",
    "test:coverage": "nx run-many --target=test --all --parallel --coverage",
    "test:e2e": "nx run-many --target=e2e --all --parallel",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "clean": "nx reset && rm -rf dist node_modules/.cache",
    "setup": "pnpm install && npm run db:setup",
    "start:prod": "node dist/apps/dashboard-api/main.js & node dist/apps/agent-api/main.js"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.16.2",
    "axios": "^1.6.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "morgan": "^1.10.1",
    "nest-winston": "^1.10.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^6.16.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-transport": "^4.9.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@nx/eslint": "21.5.3",
    "@nx/eslint-plugin": "21.5.3",
    "@nx/jest": "21.5.3",
    "@nx/js": "21.5.3",
    "@nx/nest": "21.5.3",
    "@nx/node": "21.5.3",
    "@nx/web": "21.5.3",
    "@nx/webpack": "21.5.3",
    "@nx/workspace": "21.5.3",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "20.19.9",
    "@types/passport-jwt": "^4.0.1",
    "concurrently": "^8.2.2",
    "dotenv": "^17.2.3",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "jest": "^30.0.2",
    "jest-environment-node": "^30.0.2",
    "jest-util": "^30.0.2",
    "nx": "21.5.3",
    "prettier": "^2.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.40.0",
    "webpack-cli": "^5.1.4"
  }
}
